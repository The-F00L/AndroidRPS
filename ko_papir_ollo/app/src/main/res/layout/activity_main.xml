<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <ImageView
        android:id="@+id/ImageView_Robot"
        android:layout_width="274dp"
        android:layout_height="183dp"


        app:srcCompat="@drawable/rock"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView_Human"
        android:layout_width="273dp"
        android:layout_height="172dp"
        app:srcCompat="@drawable/paper"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="308dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_Human"
        android:layout_width="313dp"
        android:layout_height="61dp"
        android:text="A te valasztasod"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_Gep"
        android:layout_width="313dp"
        android:layout_height="61dp"
        android:text="A gep valasztasa"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="493dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_papir"
        android:layout_width="101dp"
        android:layout_height="89dp"
        android:text="Papir"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="596dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_Ko"
        android:layout_width="105dp"
        android:layout_height="87dp"
        android:text="KO"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="598dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_Ollo"
        android:layout_width="103dp"
        android:layout_height="90dp"
        android:text="Ollo"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="595dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>